<div class="repo">
  <div class="info">
    <a>
      <%= repo.language %>
    </a>
    <a>
      <i class="fa fa-star-o"></i>
      <%= repo.number_of_stars %>
    </a>
    <a>
      <i class="fa fa-code-fork"></i>
      <%= repo.number_of_forks %>
    </a>
  </div>
  <h3>
   <%= link_to repo.name, repo.repo_url %>
 </h3>
</div>
